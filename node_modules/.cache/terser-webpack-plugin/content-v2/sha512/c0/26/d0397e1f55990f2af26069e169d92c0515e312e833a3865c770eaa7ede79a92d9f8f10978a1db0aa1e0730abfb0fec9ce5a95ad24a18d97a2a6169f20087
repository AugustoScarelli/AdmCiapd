horEl] = useState(null)\\r\\n  const open = Boolean(anchorEl)\\r\\n\\r\\n  const handleMenu = (e) => {\\r\\n    setAnchorEl(e.currentTarget)\\r\\n  }\\r\\n\\r\\n  const handleClose = () => {\\r\\n    setAnchorEl(null)\\r\\n  }\\r\\n\\r\\n  const handleLogout = () => {\\r\\n    logout()\\r\\n    handleClose()\\r\\n  }\\r\\n\\r\\n  const notifications = 0\\r\\n\\r\\n  return (\\r\\n    <Fragment>\\r\\n      <AppBar\\r\\n        color='primary'\\r\\n        component={Box}\\r\\n        elevation={0}\\r\\n        paddingTop={1}\\r\\n        position='sticky'\\r\\n      >\\r\\n        <Toolbar>\\r\\n          <Grid\\r\\n            alignItems='center'\\r\\n            container\\r\\n            spacing={8}\\r\\n          >\\r\\n            <Hidden smUp>\\r\\n              <Grid item>\\r\\n                <IconButton\\r\\n                  aria-label='Open drawer'\\r\\n                  color='inherit'\\r\\n                  onClick={onDrawerToggle}\\r\\n                >\\r\\n                  <MenuIcon />\\r\\n                </IconButton>\\r\\n              </Grid>\\r\\n            </Hidden>\\r\\n            <Grid item xs />\\r\\n            <Grid item>\\r\\n              <Tooltip title='Notificações'>\\r\\n                <IconButton color='inherit'>\\r\\n                  <Badge\\r\\n                    badgeContent={notifications}\\r\\n                    color='error'\\r\\n                    invisible={notifications === 0}\\r\\n                  >\\r\\n                    <NotificationsIcon />\\r\\n                  </Badge>\\r\\n                </IconButton>\\r\\n              </Tooltip>\\r\\n            </Grid>\\r\\n            <Grid item>\\r\\n              <IconButton\\r\\n                aria-haspopup='true'\\r\\n                aria-owns={open ? 'menu-appbar' : undefined}\\r\\n                color='inherit'\\r\\n                onClick={handleMenu}\\r\\n              >\\r\\n                <Avatar src={auth.info.image} />\\r\\n              </IconButton>\\r